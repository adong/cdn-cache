{
  "manifest_version": 2,
  "name": "CDNCache",
  "description": "Cache CDN files locally.",
  "version": "1.0",
  "background": {
    "scripts": [ "cdn-cache.js" ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "name": "CDNCache"
  },
  "permissions": [ "webRequest", "webRequestBlocking", "storage", "*://*/*" ]
}
